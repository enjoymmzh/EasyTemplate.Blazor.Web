{
  "App": {
    "ExpiredTime": 30 // Token过期时间，单位天
  },
  "ThemeSettings": {
    "NavTheme": "dark",
    "Layout": "side",
    "ContentWidth": "Fluid",
    "FixedHeader": false,
    "FixSiderbar": true,
    "Title": "后台管理系统",
    "PrimaryColor": "daybreak",
    "ColorWeak": false,
    "SplitMenus": false,
    "HeaderRender": true,
    "FooterRender": true,
    "MenuRender": true,
    "MenuHeaderRender": true,
    "HeaderHeight": 48
  },
  "Component": {
    "DynamicController": true,
    "Rabbitmq": false,
    "Redis": true,
    "Signalr": false,
    "Swagger": true
  },
  "Cryptogram": {
    "Enabled": false,
    "StrongPassword": false, // 是否开启密码强度验证
    "PasswordStrengthValidation": "(?=^.{6,16}$)(?=.*\\d)(?=.*\\W+)(?=.*[A-Z])(?=.*[a-z])(?!.*\\n).*$", // 密码强度验证正则表达式，必须须包含大小写字母、数字和特殊字符的组合，长度在6-16之间
    "PasswordStrengthValidationMsg": "密码必须包含大小写字母、数字和特殊字符的组合，长度在6-16之间", // 密码强度验证消息提示
    "CryptoType": "SM2", // 密码加密算法：MD5、SM2、SM4
    "Sm2PublicKey": "", //公钥
    "Sm2PrivateKey": "", //私钥
    "Sm2CompressedPublicKey": "", //压缩公钥
    "Sm4Key": "", //SM4KEY
    "Sm4IV": "", //SM4IV
    "AesKey": "", //AESKey
    "DescKey": "", //DESCKey
    "RsaPublicKey": "",
    "RsaPrivateKey": ""
  }
}
